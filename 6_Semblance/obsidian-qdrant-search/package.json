{
  "name": "obsidian-qdrant-search",
  "version": "1.0.0",
  "description": "Obsidian plugin for Qdrant semantic search",
  "main": "main.js",
  "scripts": {
    "build": "esbuild main.ts --bundle --outfile=main.js --format=cjs --platform=node --external:obsidian --external:electron --minify",
    "dev": "esbuild main.ts --bundle --outfile=main.js --format=cjs --platform=node --external:obsidian --external:electron --watch"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "obsidian": "^1.5.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
